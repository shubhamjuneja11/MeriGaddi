<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.merigaddi">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/merigaddiicon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FirstPage"
            android:label="@string/home" />
        <activity
            android:name=".Brands"
            android:label="@string/brand" />
        <activity
            android:name=".Budget"
            android:label="@string/budget" />
        <activity
            android:name=".Fuel"
            android:label="@string/fuel" />
        <activity
            android:name=".Compare"
            android:label="@string/compare" />
        <activity android:name=".Result"
            android:label="Filtered Cars"/>
        <activity android:name=".FinalResult"
            android:label="Details"/>
        <activity android:name=".CompareResult"
            android:label="Compared Result"
            ></activity>
    </application>

</manifest>